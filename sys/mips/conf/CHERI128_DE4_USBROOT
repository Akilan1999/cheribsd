#
# CHERI -- Kernel for the SRI/Cambridge "CHERI" (Capability Hardware Enhanced
# RISC Instructions) FPGA soft core, as configured in its Terasic DE-4
# reference configuration - 128 bit version.
#
# $FreeBSD$
#

include		CHERI128_DE4_SDROOT
ident		CHERI128_DE4_USBROOT

nooptions	ROOTDEVNAME
options         ROOTDEVNAME=\"ufs:/dev/da0\"
device		scbus
device		pass
device		umass
device		da
